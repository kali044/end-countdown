{
    "manifest_version": 2,
    "name": "Countdown",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.4.0.min.js", "content.js"]
        }
      ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"  
    },
    "background": {
        "scripts": ["background.js"]
      }
  }

